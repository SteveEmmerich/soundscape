model AudioTrack {
  id        String   @id @default(uuid())
  filename  String
  duration  Float
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  // Track metadata
  title     String?
  artist    String?
  album     String?

  year      Int?
  // Audio
  features AudioFeature?
  analysis   AudioAnalysis?

}